{
    "name": "entrie-cloud/laravel-cockpit",
    "description": "This is my package laravel-cockpit",
    "keywords": [
        "laravel",
        "cockpit",
        "entrie",
        "entrie-cloud",
        "laravel-cockpit",
        "cockpit-cms",
        "cockpit-package"
    ],
    "homepage": "https://github.com/entrie-cloud/laravel-cockpit",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris",
            "email": "bk@berweb.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0"
    },
    "autoload": {
        "psr-4": {
            "EntrieCloud\\LaravelCockpit\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EntrieCloud\\LaravelCockpit\\LaravelCockpitServiceProvider"
            ],
            "aliases": {
                "LaravelCockpit": "EntrieCloud\\LaravelCockpit\\Facades\\LaravelCockpit"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
